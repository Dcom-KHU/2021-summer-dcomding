    "#1주차 수업-1\n",
    "n,k=map(int,input().split())\n",
    "temp=list(map(int,input().split()))\n",
    "idx=0\n",
    "while idx!=k:\n",
    "    if idx%2==0:\n",
    "        temp.remove(min(temp))\n",
    "    else:\n",
    "        temp.remove(max(temp))\n",
    "    idx+=1\n",
    "print(sum(temp))"
   ]
  }
